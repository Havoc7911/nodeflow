# Alloy Workspace - Environment Variables Template
# Copy this file to .env and fill in your values

# Application
VITE_APP_NAME=Alloy Workspace
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# API Configuration (for future use)
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=30000

# Database (SQLite - local file path)
VITE_DB_PATH=./alloy.db

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# External Service Keys (Optional - for future integrations)
# VITE_OPENAI_API_KEY=your_key_here
# VITE_SENDGRID_API_KEY=your_key_here
# VITE_SLACK_WEBHOOK_URL=your_webhook_here

# Development
VITE_DEV_SERVER_PORT=3000
VITE_ENABLE_HMR=true

# Security
# WARNING: Never commit actual API keys or secrets!
# This file should only contain example values
